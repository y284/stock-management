<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.31.xsd">
  <changeSet id="011-supplier_commande-constraints" author="mehdi">
    <addAutoIncrement tableName="supplier_commande" columnName="id" columnDataType="BIGINT" incrementBy="1" startWith="1"/>
    <addPrimaryKey tableName="supplier_commande" columnNames="id" constraintName="pk_supplier_commande"/>
    <createIndex tableName="supplier_commande" indexName="idx_supplier_commande_uuid" unique="true">
      <column name="uuid"/>
    </createIndex>
    <addForeignKeyConstraint constraintName="fk_supplier_commande_supplier_id_supplier" baseTableName="supplier_commande" baseColumnNames="supplier_id" referencedTableName="supplier" referencedColumnNames="id"/>
    <createIndex tableName="supplier_commande" indexName="idx_supplier_commande_supplier_id">
      <column name="supplier_id"/>
    </createIndex>
    <addForeignKeyConstraint constraintName="fk_supplier_commande_warehouse_id_warehouse" baseTableName="supplier_commande" baseColumnNames="warehouse_id" referencedTableName="warehouse" referencedColumnNames="id"/>
    <createIndex tableName="supplier_commande" indexName="idx_supplier_commande_warehouse_id">
      <column name="warehouse_id"/>
    </createIndex>
  </changeSet>
</databaseChangeLog>
